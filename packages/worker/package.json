{
  "name": "@devflow/worker",
  "version": "1.0.0",
  "description": "Temporal workers for DevFlow workflows",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist *.tsbuildinfo",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "devflow",
    "temporal",
    "worker",
    "workflow",
    "ai"
  ],
  "author": "DevFlow Team",
  "license": "MIT",
  "dependencies": {
    "@devflow/common": "workspace:*",
    "@devflow/sdk": "workspace:*",
    "@prisma/client": "^5.8.1",
    "@temporalio/activity": "^1.9.0",
    "@temporalio/worker": "^1.9.0",
    "@temporalio/workflow": "^1.9.0",
    "axios": "^1.6.5",
    "dotenv": "^16.3.1",
    "redis": "^4.6.12",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/uuid": "^11.0.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
